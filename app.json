{
  "expo": {
    "name": "Gasolink",
    "slug": "gasolink",
    "version": "10.0.0",
    "orientation": "portrait",
    "userInterfaceStyle": "light",
    "scheme": "gasolink",
    "assetBundlePatterns": [
      "**/*"
    ],
    "icon": "./assets/logo.png",
    "splash": {
      "image": "./assets/logo.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "android": {
      "package": "com.dalva.gasolink",
      "adaptiveIcon": {
        "foregroundImage": "./assets/logo.png",
        "backgroundColor": "#ffffff"
      },
      "permissions": [
        "NOTIFICATIONS",
        "CAMERA",
        "RECORD_AUDIO",
        "READ_EXTERNAL_STORAGE",
        "WRITE_EXTERNAL_STORAGE",
        "FOREGROUND_SERVICE",
        "MODIFY_AUDIO_SETTINGS"
      ]
    },
    "ios": {
      "bundleIdentifier": "com.dalva.gasolink",
      "supportsTablet": true,
      "usesAppleSignIn": false,
      "infoPlist": {
        "NSUserNotificationUsageDescription": "O Gasolink usa notificações para enviar alertas de preços baixos.",
        "NSCameraUsageDescription": "O Gasolink precisa da câmera para escanear notas fiscais e gravar vídeos.",
        "NSMicrophoneUsageDescription": "O Gasolink precisa do microfone para gravar áudio nos vídeos.",
        "NSPhotoLibraryUsageDescription": "O Gasolink precisa acessar sua galeria para salvar downloads de vídeos e fotos.",
        "NSPhotoLibraryAddUsageDescription": "Permite que o Gasolink salve mídias baixadas no seu rolo de câmera."
      }
    },
    "runtimeVersion": {
      "policy": "appVersion"
    },
    "updates": {
      "fallbackToCacheTimeout": 0,
      "url": "https://u.expo.dev/5c8dfda1-4a69-4d8b-8778-33913e27792c"
    },
    "plugins": [
      "expo-router"
    ],
    "owner": "luanzimbr",
    "extra": {
      "router": {},
      "eas": {
        "projectId": "5c8dfda1-4a69-4d8b-8778-33913e27792c"
      }
    }
  }
}